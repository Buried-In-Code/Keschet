<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="Increment Minor Version" type="MavenRunConfiguration" factoryName="Maven" singleton="true">
    <MavenSettings>
      <option name="myGeneralSettings" />
      <option name="myRunnerSettings" />
      <option name="myRunnerParameters">
        <MavenRunnerParameters>
          <option name="profiles">
            <set />
          </option>
          <option name="goals">
            <list>
              <option value="build-helper:parse-version" />
              <option value="versions:set" />
              <option value="-DnewVersion=${parsedVersion.majorVersion}.${parsedVersion.nextMinorVersion}" />
              <option value="versions:commit" />
            </list>
          </option>
          <option name="pomFileName" />
          <option name="profilesMap">
            <map />
          </option>
          <option name="resolveToWorkspace" value="false" />
          <option name="workingDirPath" value="$PROJECT_DIR$" />
        </MavenRunnerParameters>
      </option>
    </MavenSettings>
    <method>
      <option name="Maven.BeforeRunTask" enabled="true" file="$PROJECT_DIR$/pom.xml" goal="test" />
    </method>
  </configuration>
</component>